      data-css="{{ section.settings["VEDA_styleBox_8"] }}"
      class="d:flex@sm ai:center jc:space-between bgc:color-light-freeze.04 p:pfs(30px,40px) mih:100%"
    >
      <div class="w:50%@sm pr:30px@sm mb:20px@+sm">
        {% if section.settings.subscribe___title %}
          <h3
            class="c:color-light-freeze fz:28px fw:400 ff:font-secondary"
            data-css="{{ section.settings["VEDA_styleBox_10"] }}"
          >
            {{ section.settings.subscribe___title }}
          </h3>
        {% endif %}
        {% if section.settings.subscribe___description %}
          <div
            class="mt:5px fz:14px c:color-light-freeze.7"
            data-css="{{ section.settings["VEDA_styleBox_12"] }}"
          >
            {{ section.settings.subscribe___description }}
          </div>
        {% endif %}
      </div>
      <div class="w:50%@sm">
        {% form 'customer' %}
          {% if builderMode and _test %}
            <div class="d:flex ai:center fz:14px p:10px m:10px_0 bgc:color-error.1 c:color-error ls:none*ul p:0*ul m:0*ul">
              <span><i class="far fa-exclamation-circle mr:10px"></i></span>
              <div class="errors">Form error placeholder</div>
            </div>
          {% elsif form.errors %}
            <div class="d:flex ai:center fz:14px p:10px m:10px_0 bgc:color-error.1 c:color-error ls:none*ul p:0*ul m:0*ul">
              <span><i class="far fa-exclamation-circle mr:10px"></i></span>
              {{ form.errors | default_errors }}
            </div>
          {% endif %}

          {% if builderMode and _test %}
            <div class="d:flex ai:center fz:14px p:10px m:10px_0 bgc:color-success.1 c:color-success">
              <span><i class="far fa-check-circle mr:10px"></i></span>
              <div>{{ 'veda.newsletter.success' | t }}</div>
            </div>
          {% elsif form.posted_successfully? %}
            <div class="d:flex ai:center fz:14px p:10px m:10px_0 bgc:color-success.1 c:color-success">
              <span><i class="far fa-check-circle mr:10px"></i></span>
              <div>{{ 'veda.newsletter.success' | t }}</div>
            </div>
          {% endif %}

          <div class="d:flex w:100% h:40px c:color-gray9 bgc:transparent bdb:2px_solid_color-light-freeze.6">
            <input type="hidden" name="contact[tags]" value="newsletter">
            <input
              class="w:100% h:100%! pr:15px bgc:transparent c:color-light-freeze c:color-light-freeze.8||ph fz:16px ff:font-secondary! bd:none"
              type="email"
              name="contact[email]"
              autocorrect="off"
              autocapitalize="off"
              placeholder="{{ 'veda.templates.contact.form.email' | t }}"
              data-css="{{ section.settings["VEDA_styleBox_14"] }}"
            >
            <button
              class="d:inline-flex ai:center h:100% ff:font-secondary fw:500 fz:16px trs:0.3s cur:pointer c:color-light-freeze trs:0.3s bgc:transparent bd:0 bxsh:none o:none"
              type="submit"
              aria-label="submit"
              data-css="{{ section.settings["VEDA_styleBox_16"] }}"
            >
              <span
                class="mr:10px"
                data-css="{{ section.settings["VEDA_styleBox_18"] }}"
              >
                <i class="far fa-paper-plane"></i>
              </span>
              <span>{{ 'veda.templates.contact.form.send' | t }}</span>
            </button>
          </div>
        {% endform %}
      </div>
    </div>
  {% endif %}
{% endcapture %}

