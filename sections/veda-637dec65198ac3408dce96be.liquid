              class="d:flex ai:center c:color-light-freeze.6 fz:14px ff:font-secondary pt:6px pb:6px trs:0.3s"
              href="mailto:veda@email.com?subject=&body="
              data-css=""
            >
              <span class="mr:5px"><i class="far fa-envelope"></i></span>
              <span class="pos:relative"> veda@email.com </span>
            </a>
          </li>
        </ul>
      </div>
    </div>
  {% endif %}
{% endcapture %}

{% capture FooterBottom %}
  <div
    class="ff:font-secondary c:color-light-freeze.8 fz:14px bdt:1px_solid_color-light-freeze.1 mt:30px"
    data-css="{{ section.settings["VEDA_styleBox_6"] }}"
  >
    <div class="d:flex fld:column@+sm ai:center jc:space-between p:20px_0">
      <div class="flx:1 d:flex@sm">{{ Socials }}</div>
      <div class="mt:15px@+sm flx:1 ta:center">
        {{ section.settings.VEDA_richText_1 }}
      </div>
      <div class="pos:relative flx:1 d:flex ai:center jc:flex-end c:color-gray7 mt:15px@+sm">
        {{ LanguageAndCurrency }}
      </div>
    </div>
  </div>
{% endcapture %}

{% capture FooterTop %}
  {% if section.settings.information___enable or section.settings.subscribe___enable %}
    <div
      {% if section.settings.information___enable and section.settings.subscribe___enable %}
        class="row row-cols-lg-2 row-cols-md-1 row-cols-sm-1 row-cols-1"
      {% else %}
        class="row row-cols-1"
      {% endif %}
      {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
        style="{{ rowGap }}"
      {% endif %}
    >
      <div
        class="col"
        {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
          style="{{ colGap }}"
        {% endif %}
      >
        {{ Information }}
      </div>
      <div
        class="col"
        {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
          style="{{ colGap }}"
        {% endif %}
      >
        {{ Subscribe }}
      </div>
    </div>
    <div class="h:40px"></div>
  {% endif %}
{% endcapture %}

<footer
  data-id="{{ uniqueId }}"
  class="veda-lazyload pos:relative z:1 bgz:cover bgp:center bdt:1px_solid_transparent"
  style="{{ backgroundColor }}{{ backgroundImage }}{{ sectionPadding }}{{ sectionSeparator }}"
>
  <div
    class="{{ containerType }}"
    {% if section.settings.VEDA_slider_1
      or section.settings.VEDA_slider_1 == 0
      or section.settings.section_settings___width
      or section.settings.section_settings___width == 0
    %}
      style="{{ containerGap }}{{ containerWidth }}"
    {% endif %}
  >
    {{ FooterTop }}
    <div
      class="row row-cols-lg-{{ section.settings["column__lg"] }} row-cols-md-{{ section.settings["column__md"] }} row-cols-sm-{{ section.settings["column__sm"] }} row-cols-{{ section.settings["column__xs"] }}"
      {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
        style="{{ rowGap }}"
      {% endif %}
    >
      {{ Navigation }}
    </div>
    {{ FooterBottom }}
  </div>
  {% if section.settings.VEDA_select_2 == 'video' and section.settings.VEDA_video_1 %}
    <div data-veda-video="{{ section.settings["VEDA_video_1"] }}"></div>
  {% endif %}
  {% if section.settings.VEDA_switch_2 %}
    <div
      class="pos:absolute ins:0 z:-1"
      style="background-color: {{ section.settings["VEDA_color_2"] }};backdrop-filter: blur({{ section.settings["VEDA_slider_3"] }}px)"
    ></div>
  {% endif %}
</footer>
