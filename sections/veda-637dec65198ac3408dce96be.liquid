{% assign uniqueId = 'id_ef9599d5-dd2a-462b-b79d-3adf79ab8b83' %}
{% assign builderMode = false %}
{% assign envatoMode = false %}
{% liquid
  if section.settings.VEDA_switch_1
    assign backgroundFixed = 'fixed'
  else
    assign backgroundFixed = 'scroll'
  endif

  if section.settings.VEDA_select_2 == 'color'
    assign backgroundColor = 'background-color: ' | append: section.settings.VEDA_color_1 | append: ';'
  else
    assign backgroundColor = ''
  endif

  if section.settings.VEDA_select_2 == 'image' and section.settings.VEDA_image_2
    assign bgImage = section.settings.VEDA_image_2 | append: '&width=2000'

    assign backgroundImage = 'background-image: url(' | append: bgImage | append: ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: section.settings.VEDA_select_3 | append: ';'
  elsif section.settings.VEDA_select_2 == 'image' and section.settings.VEDA_image_2 == blank
    assign bgImage = 'https://cdn.shopify.com/s/files/1/0552/5510/5616/files/udxhtnzxtq.png?v=1681903122&width=1000'

    assign backgroundImage = 'background-image: url(' | append: bgImage | append: ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: section.settings.VEDA_select_3 | append: ';'
  else
    assign backgroundImage = ''
  endif

  assign sectionPadding = 'padding-top: ' | append: section.settings.VEDA_space_4 | append: 'px;padding-right: ' | append: section.settings.VEDA_space_3 | append: 'px;padding-bottom: ' | append: section.settings.VEDA_space_1 | append: 'px;padding-left: ' | append: section.settings.VEDA_space_2 | append: 'px;'

  if section.settings.VEDA_select_1 == 'full_width'
    assign containerType = 'container-fluid'
  else
    assign containerType = 'container'
  endif

  if section.settings.VEDA_slider_1 >= 0
    assign containerGap = 'padding-left: ' | append: section.settings.VEDA_slider_1 | append: 'px; padding-right: ' | append: section.settings.VEDA_slider_1 | append: 'px;'
  else
    assign containerGap = ''
  endif

  if section.settings.section_settings___width >= 0
    assign containerWidth = 'max-width: ' | append: section.settings.section_settings___width | append: 'px;'
  else
    assign containerWidth = ''
  endif

  if section.settings.VEDA_slider_2 >= 0
    assign settingGap = section.settings.VEDA_slider_2 | divided_by: 2.0

    assign settingGapVertical = section.settings.VEDA_slider_2
  else
    assign settingGap = ''

    assign settingGapVertical = ''
  endif

  assign colGap = 'margin-top: ' | append: settingGapVertical | append: 'px; padding-left: ' | append: settingGap | append: 'px; padding-right: ' | append: settingGap | append: 'px;'

  assign rowGap = 'margin-top: -' | append: settingGapVertical | append: 'px; margin-left: -' | append: settingGap | append: 'px; margin-right: -' | append: settingGap | append: 'px;'

  assign sectionSeparator = ''

  if section.settings.VEDA_switch_3
    if section.settings.VEDA_slider_4
      assign sectionSeparator = sectionSeparator | append: 'border-top: ' | append: section.settings.VEDA_slider_4 | append: 'px solid ' | append: section.settings.VEDA_color_3 | append: ';'
    endif

    if section.settings.VEDA_slider_5
      assign sectionSeparator = sectionSeparator | append: 'border-bottom: ' | append: section.settings.VEDA_slider_5 | append: 'px solid ' | append: section.settings.VEDA_color_3 | append: ';'
    endif
  endif

  if colGap and rowGap and builderMode and envatoMode
  endif
%}

{% liquid
  assign _alt = false

  assign _style = false

  assign _placeholder = false

  assign _placeholder_class = false

  assign _component = false

  assign _class = false

  assign _test = false

  assign _icon_class = false
%}
{% capture LanguageAndCurrency %}
  {% for block in section.blocks %}
    {% assign lcItem = block.settings %}

    {% if lcItem.VEDA_switch_5 %}
      {% if block.type == 'VEDA_array_1' %}
        <div
          {% if builderMode %}
            data-tooltip-position="left"
            data-tooltip-text="When you use the editor this data will be fake"
          {% endif %}
        >
          {% liquid
            assign localization_id = 'localization_form_' | append: uniqueId
          %}
          {% form 'localization', id: localization_id, class: 'localization_form' %}
            <input type="hidden" name="locale_code" value="{{ form.current_locale.iso_code }}">
            <div class="veda-select veda-select-lang">
              <div
                data-css="{{ lcItem.VEDA_styleBox_26 }}"
                class="veda-select__view c:color-light-freeze d:flex ai:center jc:space-between bd:0! p:5px_0! fz:14px"
              >
                <div class="veda-select__label ff:font-secondary"></div>
                <span class="pt:2px pl:4px"><i class="far fa-angle-down"></i></span>
              </div>
              <div
                data-css="{{ lcItem.VEDA_styleBox_28 }}"
                class="veda-select__options bgc:color-light w:fit-content! bxsh:0_5px_15px_color-gray9.2 bdrs:5px ov:hidden bd:0! p:10px_0 t:auto! b:100%!"
              >
                {% for language in localization.available_languages %}
                  {% liquid
                    assign defaultClass = 'veda-select__option whs:nowrap bd:0! ff:font-secondary fz:14px p:5px_15px bgc:transparent trs:0.3s'
                  %}
                  <div
                    data-value="{{ language.iso_code }}"
                    {% if language.iso_code == localization.language.iso_code %}
                      {% if builderMode %}
                        class="{{ defaultClass }}"
                      {% else %}
                        class="{{ defaultClass }} c:color-primary"
                      {% endif %}
                      data-selected="selected"
                    {% else %}
                      class="{{ defaultClass }} c:color-primary|h"
                    {% endif %}
                  >
                    {{ language.endonym_name }}
                  </div>
                {% endfor %}
              </div>
            </div>
          {% endform %}
        </div>
      {% elsif block.type == 'VEDA_array_2' %}
        {% liquid
          assign currencySize = shop.enabled_currencies | size
        %}
        {% if currencySize > 1 %}
          <div
            {% if builderMode %}
              data-tooltip-position="left"
              data-tooltip-text="When you use the editor this data will be fake"
            {% endif %}
          >
            {% liquid
              assign currency_id = 'currency_form_' | append: uniqueId
            %}
            {% form 'localization', id: currency_id, class: 'currency_form' %}
              <input
                type="hidden"
                name="currency_code"
                value="{{ form.current_currency.iso_code }}"
              >
              <div class="veda-select veda-select-currency">
                <div
                  data-css="{{ lcItem.VEDA_styleBox_26 }}"
                  class="veda-select__view c:color-light-freeze d:flex ai:center jc:space-between bd:0! p:5px_0! fz:14px"
                >
                  {% if builderMode %}
                    <span class="mr:5px flag flag-round flag-md flag-USD"></span>
                    <div class="veda-select__label ff:font-secondary">USD</div>
                  {% else %}
                    <span class="mr:5px flag flag-round flag-md flag-{{ form.current_currency.iso_code }}"></span>
                    <div class="veda-select__label ff:font-secondary">{{ form.current_currency.iso_code }}</div>
                  {% endif %}
                  <span class="pt:2px pl:4px"><i class="far fa-angle-down"></i></span>
                </div>
                <div
                  data-css="{{ lcItem.VEDA_styleBox_28 }}"
                  class="veda-select__options bgc:color-light w:fit-content! bxsh:0_5px_15px_color-gray9.2 bdrs:5px ov:hidden bd:0! p:10px_0 t:auto! b:100%!"
                >
                  {% for currency in shop.enabled_currencies %}
                    <div
                      data-value="{{ currency.iso_code }}"
                      {% if form.current_currency.iso_code == currency.iso_code %}
                        class="veda-select__option whs:nowrap bd:0! ff:font-secondary fz:14px p:5px_15px d:flex ai:center bgc:transparent trs:0.3s c:color-primary"
                        data-selected="selected"
                      {% elsif builderMode and currency.iso_code == 'USD' %}
                        class="veda-select__option whs:nowrap bd:0! ff:font-secondary fz:14px p:5px_15px d:flex ai:center bgc:transparent trs:0.3s c:color-primary|h"
                        data-selected="selected"
                      {% else %}
                        class="veda-select__option whs:nowrap bd:0! ff:font-secondary fz:14px p:5px_15px d:flex ai:center bgc:transparent trs:0.3s c:color-primary|h"
                      {% endif %}
                    >
                      <span class="mr:5px flag flag-round flag-md flag-{{ currency.iso_code }}"></span>
                      {{ currency.iso_code }}
                    </div>
                  {% endfor %}
                </div>
              </div>
            {% endform %}
          </div>
        {% endif %}
      {% elsif block.type == 'link' %}
        <a
          class="c:color-light-freeze"
          data-css="{{ lcItem.link_design.id }}"
          href="{{ lcItem.link }}"
        >
          {% if lcItem.enable_icon %}
            <span class="mr:5px">{{ lcItem.link_icon }}</span>
          {% endif %}
          <span>{{ lcItem.link_label }}</span>
        </a>
      {% elsif block.type == 'VEDA_array_3' %}
        <div
          class="d:flex jc:center"
          style="width: {{ lcItem.VEDA_slider_6 }}px; height: {{ lcItem.VEDA_slider_7 }}px"
        >
          {% if lcItem.VEDA_select_4 == 'line' %}
            <div class="h:100% w:1px" style="background-color: {{ lcItem.VEDA_color_4 }}"></div>
          {% endif %}
        </div>
      {% endif %}
    {% endif %}
  {% endfor %}
{% endcapture %}

{% capture Socials %}
  {% if section.settings.VEDA_switch_4 %}
    <div class="d:flex mt:15px@+sm">
      {% for block in section.blocks %}
        {% if block.type == 'socials' %}
          {% assign social = block.settings %}

          <a
            href="{{ social.socials___link }}"
            data-css="{{ social.socials___design__id }}"
            class="fz:16px c:color-light-freeze.8 c:color-primary-freeze|h! trs:0.3s ml:15px"
          >
            {{ social.socials___icon }}
          </a>
        {% endif %}
      {% endfor %}
    </div>
  {% endif %}
{% endcapture %}

{% capture Information %}
  {% if section.settings.information___enable %}
    <div
      data-css="{{ section.settings["VEDA_styleBox_20"] }}"
      class="d:flex@sm ai:center jc:space-between bgc:color-light-freeze.04 p:pfs(30px,40px) mih:100%"
    >
      {% if section.settings.information___logo__src %}
        <div
          class="pos:relative flxs:1 mr:30px@sm mb:20px@+sm"
          style="width: {{ section.settings["information___logo_width"] }}px"
        >
          {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
          {% if section.settings.information___logo__src != blank %}
            <img
              class="veda-lazyload {{ _class }}"
              src="{{ section.settings["information___logo__src"] | append: '&width=10' }}"
              draggable="false"
              {% if _alt %}
                alt="{{ _alt }}"
              {% else %}
                alt="image"
              {% endif %}
              {% if section.settings.VEDA_image_5 %}
                data-intrinsic-width="{{ section.settings["VEDA_image_5"] }}"
                width="{{ section.settings["VEDA_image_5"] }}"
              {% endif %}
              {% if section.settings.VEDA_image_4 %}
                height="{{ section.settings["VEDA_image_4"] }}"
                data-intrinsic-height="{{ section.settings["VEDA_image_4"] }}"
              {% endif %}
              {% if _style %}
                style="{{ _style }}"
              {% endif %}
              loading="lazy"
            >
          {% else %}
            {% liquid
              assign pl = 'lifestyle-1'

              if _placeholder
                assign pl = _placeholder
              endif

              assign placeholder_class = 'veda-placeholder-svg bgc:color-gray2 ' | append: _class

              if _placeholder_class
                assign placeholder_class = 'veda-placeholder-svg bgc:color-gray2 ' | append: _placeholder_class
              endif
            %}
            {{ pl | placeholder_svg_tag: placeholder_class }}
          {% endif %}
          {% comment %}KET_THUC_TACH_FILE{% endcomment %}
        </div>
      {% endif %}
      <div class="flx:1">
        {% if section.settings.information___title %}
          <h3
            class="c:color-light-freeze fz:28px fw:400 ff:font-secondary"
            data-css="{{ section.settings["VEDA_styleBox_22"] }}"
          >
            {{ section.settings.information___title }}
          </h3>
        {% endif %}
        {% if section.settings.VEDA_richText_2 %}
          <div
            class="mt:5px fz:14px c:color-light-freeze.7"
            data-css="{{ section.settings["VEDA_styleBox_24"] }}"
          >
            {{ section.settings.VEDA_richText_2 }}
          </div>
        {% endif %}
      </div>
    </div>
  {% endif %}
{% endcapture %}

{% capture Subscribe %}
  {% if section.settings.subscribe___enable %}
