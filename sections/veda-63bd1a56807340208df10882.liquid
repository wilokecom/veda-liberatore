<section
  data-id="{{ uniqueId }}"
  class="veda-lazyload pos:relative z:1 z:2|h bgz:cover bgp:center"
  style="{{ backgroundColor }}{{ backgroundImage }}{{ sectionPadding }}{{ sectionSeparator }}"
>
  <div
    class="{{ containerType }}"
    {% if section.settings.VEDA_slider_1
      or section.settings.VEDA_slider_1 == 0
      or section.settings.section_settings___width
      or section.settings.section_settings___width == 0
    %}
      style="{{ containerGap }}{{ containerWidth }}"
    {% endif %}
  >
    <div
      class="row"
      {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
        style="{{ rowGap }}"
      {% endif %}
    >
      <div
        class="col-md-12 col-lg-2"
        {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
          style="{{ colGap }}"
        {% endif %}
      >
        <nav class="veda-nav">
          <h2
            data-css="{{ section.settings["VEDA_styleBox_30"] }}"
            class="pos:relative p:10px_15px pr:30px@+md bgc:color-primary c:color-light-freeze fz:14px fw:500 bdrs:6px bdrs:6px_6px_0_0@md m:0"
          >
            <span>{{ section.settings.navigation___title }}</span>
            <span class="veda-nav__hamburger veda-hamburger-menu-icon pos:absolute ins:0 p:10px_15px z:9 d:flex ai:center c:color-light-freeze fz:16px ta:right d:none">
              <i class="far fa-bars"></i>
            </span>
            <span
              class="pos:absolute l:10px b:-6px w:12px h:6px bgc:inherit d:none@+md"
              style="clip-path: polygon(50% 100%, 0 0, 100% 0);"
            ></span>
          </h2>
          <div
            data-css="{{ section.settings["VEDA_styleBox_26"] }}"
            class="bd:1px_solid_color-gray3@md bdrs:0_0_6px_6px"
          >
            {{ Navigation }}
          </div>
        </nav>
      </div>
      <div
        class="col-md-9 col-lg-8"
        {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
          style="{{ colGap }}"
        {% endif %}
      >
        {{ Content }}
      </div>
      <div
        class="col-md-3 col-lg-2"
        {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
          style="{{ colGap }}"
        {% endif %}
      >
        {% for block in section.blocks %}
          {% if block.type == 'banner' %}
            {% assign image = block.settings %}

            <a
              href="{{ image.banner___link }}"
              class="veda-lazyload d:block mb:20px mb:0!|lc mb:var(--current-mb)"
              {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
                style="--current-mb: {{ section.settings["VEDA_slider_2"] }}px"
              {% else %}
                style="--current-mb: 20px"
              {% endif %}
            >
              {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
              {% if image.banner___image__src != blank %}
                <img
                  class="veda-lazyload {{ _class }}"
                  src="{{ image.banner___image__src | append: '&width=10' }}"
                  draggable="false"
                  {% if _alt %}
                    alt="{{ _alt }}"
                  {% else %}
                    alt="image"
                  {% endif %}
                  {% if image.banner___image__width %}
                    data-intrinsic-width="{{ image.banner___image__width }}"
                    width="{{ image.banner___image__width }}"
                  {% endif %}
                  {% if image.banner___image__height %}
                    height="{{ image.banner___image__height }}"
                    data-intrinsic-height="{{ image.banner___image__height }}"
                  {% endif %}
                  {% if _style %}
                    style="{{ _style }}"
                  {% endif %}
                  loading="lazy"
                >
              {% else %}
                {% liquid
                  assign pl = 'lifestyle-1'

                  if 'image'
                    assign pl = 'image'
                  endif

                  assign placeholder_class = 'veda-placeholder-svg bgc:color-gray2 ' | append: _class

                  if _placeholder_class
                    assign placeholder_class = 'veda-placeholder-svg bgc:color-gray2 ' | append: _placeholder_class
                  endif
                %}
                {{ pl | placeholder_svg_tag: placeholder_class }}
              {% endif %}
              {% comment %}KET_THUC_TACH_FILE{% endcomment %}
            </a>
          {% endif %}
        {% endfor %}
      </div>
    </div>
  </div>
  {% if section.settings.VEDA_select_2 == 'video' and section.settings.VEDA_video_1 %}
    <div data-veda-video="{{ section.settings["VEDA_video_1"] }}"></div>
  {% endif %}
  {% if section.settings.VEDA_switch_2 %}
    <div
      class="pos:absolute ins:0 z:-1"
      style="background-color: {{ section.settings["VEDA_color_2"] }};backdrop-filter: blur({{ section.settings["VEDA_slider_3"] }}px)"
    ></div>
  {% endif %}
</section>
